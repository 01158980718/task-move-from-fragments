<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/goFragment">

    <fragment
        android:id="@+id/goFragment"
        android:name="com.example.myapplication99.GoFragment"
        android:label="fragment_go"
        tools:layout="@layout/fragment_go" >
        <action
            android:id="@+id/action_goFragment_to_backFragment"
            app:destination="@id/backFragment"
            app:launchSingleTop="true" />
    </fragment>
    <fragment
        android:id="@+id/backFragment"
        android:name="com.example.myapplication99.BackFragment"
        android:label="fragment_back"
        tools:layout="@layout/fragment_back" />
</navigation>